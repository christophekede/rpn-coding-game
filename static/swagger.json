       "paths": {
          "/rpn/op": {
              "get" :{
                  "summary": "List all the operand",
                  "operationId":"getOp",
                 
                  "responses": {
                  "200": {
                      "description": "OK"
                     
                  },
                  "404": {
                      "description": "NOT FOUND"
                     
              }
              }
              }
          },
          "/rpn/op/{op}/stack/{stack_id}":{
            "post": {
        
                "operationId":"operand",
                "parameters": [{
                    "name": "stack_id",
                    "in": "path",
                    "description": "stack id to delete",
                    "required": true,
                    "schema" : {
                        "$ref": "#/components/schemas/stack_id"
                    }
                },
                {
                    "name": "op",
                    "in": "path",
                    "description": "operand to apply: +,-,*,/",
                    "required": true,
                    "schema" : {
                        "$ref": "#/components/schemas/operand"
                    }
                }
            ],
           
                "summary": "Apply an operand to a stack",
                
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "NOT FOUND"
                       
                }
                }
            }
          },
          "/rpn/stack":{
  
              "post" :{
               
                  "operationId":"postStack",
                  "summary": "Create a new stack",
                  "responses": {
                  "200": {
                      "description": "OK"
                  },
                  "404": {
                      "description": "NOT FOUND"
                      
              }
              }
              },
             
              "get": {
                          
