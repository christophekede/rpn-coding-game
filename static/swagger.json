                "parameters": [{
                    "name": "stack_id",
                    "in": "path",
                    "description": "stack id to delete",
                    "required": true,
                    
                    "schema" : {
                        "$ref": "#/components/schemas/stack_id"
                    }
                }],
                "summary": "Delete a stack",
                
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "NOT FOUND"
                   }
                }
            },
            "post": {
        
                "operationId":"addStack",
                "parameters": [{
                    "name": "stack_id",
                    "in": "path",
                    "description": "stack id to delete",
                    "required": true,
                    "schema" : {
                        "$ref": "#/components/schemas/stack_id"
                    }
                }],
                "requestBody": {
                    "description": "Post New todo To Database",
                    "required": true,
                    "content": {
                        "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/value"
                            },
                         
                         "example":{
                            "value":1
                         }
                        
                        }
                    }
                },
